run str {
  time t &= DateTime(2019, 2, 14, 9, 10, 3)
  t.UTC = true
  str out = Format(`YYYY-MM-DD HH:mm:ss tz zz ddd MMMM`, UTC(t))
  t &= DateTime(1903, 7, 5, 16, 11, 5)
  t.UTC = true
  return out + Format(`MDYY h:m:s pm z dddd MMM`, t)
}
